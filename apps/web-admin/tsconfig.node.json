{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@gx/tsconfig/node.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "types": ["@gx/types/mock", "@gx/types/global"],
    "noEmit": false
  },
  "include": [
    "vite.config.ts",
    "types/*.d.ts",
    "mock/**/*.ts",
    "config/*.ts",
    "plugin/**/*.ts"
  ]
}
