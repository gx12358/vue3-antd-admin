{
  "name": "vue-gx-monorepo",
  "type": "module",
  "version": "4.0.0",
  "private": true,
  "author": {
    "name": "gx12358",
    "email": "gx12358@gmail.com",
    "url": "https://github.com/gx12358"
  },
  "license": "MIT",
  "homepage": "https://github.com/gx12358/vue3-antd-admin",
  "repository": "gx12358/vue3-antd-admin.git",
  "bugs": "https://github.com/gx12358/vue3-antd-admin/issues",
  "keywords": [
    "monorepo",
    "turbo",
    "gx12358",
    "gx admin",
    "gx pro",
    "vue",
    "vue admin",
    "vue gx admin",
    "vue gx admin pro",
    "vue3"
  ],
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "build:admin": "pnpm run build --filter=@gx/web-admin",
    "build:play": "pnpm run build --filter=@gx/playground",
    "build:docker": "./scripts/deploy/build-local-docker-image.sh",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "check:type": "turbo run typecheck",
    "clean": "node ./scripts/clean.mjs",
    "commit": "czg",
    "dev": "turbo-run dev",
    "dev:admin": "pnpm -F @gx/web-admin run dev",
    "dev:h5": "pnpm -F @gx/web-h5 run dev",
    "dev:play": "pnpm -F @gx/playground run dev",
    "lint": "pnpm lint:eslint",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "preview": "turbo-run preview",
    "publint": "vsh publint",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "test:e2e": "turbo run test:e2e",
    "update:deps": "npx taze -r -w",
    "version": "pnpm exec changeset version && pnpm install --no-frozen-lockfile",
    "catalog": "pnpx codemod pnpm/catalog"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@commitlint/cli": "catalog:",
    "@commitlint/config-conventional": "catalog:",
    "@commitlint/types": "catalog:",
    "@gx/tsconfig": "workspace:*",
    "@gx/turbo-run": "workspace:*",
    "@gx/vite-config": "workspace:*",
    "@gx/vsh": "workspace:*",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "autoprefixer": "^10.4.21",
    "cross-env": "catalog:",
    "husky": "catalog:",
    "lefthook": "catalog:",
    "lint-staged": "catalog:",
    "rimraf": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "@ast-grep/napi": "catalog:",
      "esbuild": "0.25.3",
      "pinia": "catalog:",
      "vue": "catalog:"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  }
}
