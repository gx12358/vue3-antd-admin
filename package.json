{
  "name": "gx-pro-admin",
  "version": "2.0.0",
  "scripts": {
    "bootstrap": "pnpm i",
    "serve": "pnpm run dev",
    "dev": "pnpm run start:dev",
    "start:dev": "cross-env VITE_APP_ENV=dev vite",
    "start:production": "cross-env VITE_APP_ENV=dev vite --mode production",
    "start:pro": "cross-env VITE_APP_ENV=dev vite --mode pro",
    "build": "cross-env vite build && esno ./build/script/postBuild.ts",
    "build:pro": "cross-env vite build --mode pro && esno ./build/script/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && pnpm run build",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm run bootstrap",
    "prepare": "husky install"
  },
  "dependencies": {
    "@gx-design-vue/pro-utils": "^0.2.0-beta.26",
    "@gx-design-vue/pro-hooks": "^0.2.0-beta.19",
    "@gx-design-vue/pro-provider": "^0.1.0-beta.49",
    "@gx-design-vue/pro-app": "^0.1.0-beta.10",
    "@gx-design-vue/pro-table": "^0.2.0-beta.46",
    "@gx-design-vue/image": "^0.2.0-beta.14",
    "@gx-design-vue/pro-card": "^0.2.0-beta.21",
    "@gx-design-vue/scrollbar": "^0.2.0-beta.12",
    "@gx-design-vue/pro-modal": "^0.2.0-beta.14",
    "@gx-design-vue/pro-watermark": "^0.2.0-beta.6",
    "@gx-design-vue/pro-layout": "^0.1.0-beta.48",
    "@gx-design-vue/pro-form": "^0.2.0-beta.17",
    "@vueuse/core": "^10.7.2",
    "@vueuse/shared": "^10.7.2",
    "ant-design-vue": "^4.1.2",
    "ali-oss": "^6.17.1",
    "axios": "^1.6.7",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.2.1",
    "nprogress": "^0.2.0",
    "pinia": "2.1.7",
    "tinymce": "^5.10.3",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@purge-icons/generated": "^0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.10",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^20.11.19",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "3.2.37",
    "autoprefixer": "^10.4.5",
    "body-parser": "^1.19.0",
    "terser": "^5.14.2",
    "unocss": "^0.58.5",
    "mockjs": "^1.1.0",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "connect": "^3.7.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "vue-eslint-parser": "^9.4.2",
    "esno": "^0.7.3",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.0",
    "less": "^4.1.2",
    "lint-staged": "12.3.7",
    "path-to-regexp": "^6.2.1",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-node": "^10.1.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "0.25.2",
    "vite": "^5.1.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.19.0",
    "vite-plugin-vue-setup-extend": "0.4.0",
    "vue-tsc": "^1.8.27"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
